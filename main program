#In this program you can calculate Result of engineering years of 8 sem in a single program   
def sem1() : #function of sem 1 
     global s1,s2,s3,s4,s15,s6,p1,p2,p3,p4 #define variables as global
     print("\n ----------------------------\n | no.      List of subject | \n ----------------------------\n | Enter the marks below    | ") 
     s1=int(input(" | 1.    Mathematics - 1(T) = 100/"))
     while s1>100 :# limiting of marks by using while loop
         print(" | Invalid input")
         s1=int(input(" | 1.    Mathematics - 1(T) = 100/"))
     s2=int(input(" | 2.       Engg Physics(T) = 100/"))
     while s2>100 :
         print(" | Invalid input")
         s2=int(input(" | 2.       Engg Physics(T) = 100/"))
     p1=int(input(" | 3.       Engg Physics(P) = 50/"))
     while p1>50 :
         print(" | Invalid input")
         p1=int(input(" | 3.       Engg Physics(P) = 50/"))
     s3=int(input(" | 4.     Engg Chemistry(T) = 100/"))
     while s3>100 :
         print(" | Invalid input")
         s3=int(input(" | 4.     Engg Chemistry(T) = 100/"))
     p2=int(input(" | 5.     Engg Chemistry(P) = 50/"))
     while p2>50 :
         print(" | Invalid input")
         p2=int(input(" | 5.     Engg Chemistry(P) = 50/"))
     s4=int(input(" | 6.                BEE(T) = 100/"))
     while s4>100 :
         print(" | Invalid input")
         s4=int(input(" | 6.                BEE(T) = 100/"))
     p3=int(input(" | 7.                BEE(P) = 50/"))
     while p3>50 :
         print(" | Invalid input")
         p3=int(input(" | 7.                BEE(P) = 50/"))
     s15=int(input(" | 8.         Civil Engg(T) = 100/"))
     while s15>100 :
         print(" | Invalid input")
         s15=int(input(" | 8.         Civil Engg(T) = 100/"))
     s6=int(input(" | 9.     Engg Graphics (T) = 100/"))
     while s6>100 :
         print(" | Invalid input")
         s6=int(input(" | 9.     Engg Graphics (T) = 100/"))
     p4=int(input(" | 10.    Engg Graphics (P) = 50/"))
     while p4>50 :
         print(" | Invalid input")
         p4=int(input(" | 10.    Engg Graphics (P) = 50/"))
