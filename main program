def main(): # main function to find result
 print("\n ----------------------------\n | List of choice        no.| \n ----------------------------\n | First year          -> 1.| \n | Second year         -> 2.| \n | Third year          -> 3.| \n | Forth year          -> 4.| ")
 ch1=int(input(" | Enter your choice = "))
 while ch1>4 or ch1<=0  : # limiting range of 1 to 4 using while loop 
          print(" | Invalid input")
          ch1=int(input(" | Enter your choice = "))
 if ch1==1:  #select 1st year
  print("\n\n ----------------------------\n | List of semester      no.| \n ----------------------------\n | First sem           -> 0.| \n | Second sem          -> 1.| ")
  ch2=int(input(" | Enter your choice = "))
  while ch2>1 :
     print(" | Invalid input")
     ch2=int(input(" | Enter your choice = "))
  if ch1==1 and ch2==0: #selection of 1st sem 1st year
   sem1()
   print("\n\n ----------------------------\n |          RESULT          | \n ----------------------------")
   ch3=int(ch1)+int(ch2) #adding ch1 and ch2 to get sem in numbers
   print(" |   Of",ch1,"year",ch3,"semester   |" )
   if s1 and s2 and s3 and s4 and s15 and s6>= 40 and p1 and p2 and p3 and p4 >= 30 : # calling variables from sem1()
    print(" |      RESULT IS PASS      | ")
    print(" |      PERCENTAGE %        = ",(s1+s2+s3+s4+s15+s6+p1+p2+p3+p4)/8) #percent calculation 
    print(" |         CGPA             = ",((s1+s2+s3+s4+s15+s6+p1+p2+p3+p4)/80)+0.75,"\n ----------------------------")# cgpa calculation
   else:
    print(" |      RESULT IS FAIL      | \n ----------------------------")
  if ch1==1 and ch2==1: #selection of 2nd sem 1st year
   sem2()
   print("\n\n ----------------------------\n |          RESULT          | \n ----------------------------")
   ch3=int(ch1)+int(ch2)
   print(" |   Of",ch1,"year",ch3,"semester   |" )
   if s1 and s2 and s3 and s4 and s25 >= 40 and p1 and p2 and p3 and p4 >= 30 : # calling variables from sem2()
    print(" |      RESULT IS PASS      | ")
    print(" |      PERCENTAGE %        = ",(s1+s2+s3+s4+s25+p1+p2+p3+p4)/7)
    print(" |         CGPA             = ",((s1+s2+s3+s4+s25+p1+p2+p3+p4)/70)+0.75,"\n ----------------------------")
   else:
    print(" |      RESULT IS FAIL      | \n ----------------------------")
 if ch1==2: #second yaer result calc.
  print("\n\n ----------------------------\n | List of semester      no.| \n ----------------------------\n | Third sem           -> 1.| \n | forth sem           -> 2.| ")
  ch2=int(input(" | Enter your choice = "))
  while ch2>2 :
     print(" | Invalid input")
     ch2=int(input(" | Enter your choice = "))
  if ch1==2 and ch2==1:#selection of 3rd sem 2nd year
   sem3()
   print("\n\n ----------------------------\n |          RESULT          | \n ----------------------------")
   ch3=int(ch1)+int(ch2)
   print(" |   Of",ch1,"year",ch3,"semester   |" )
   if s1 and s2 and s3 and s4 and s35>= 40 and p1 and p2 and p3>= 30 : # calling variables from sem3()
    print(" |      RESULT IS PASS      | ")
    print(" |      PERCENTAGE %        = ",(s1+s2+s3+s4+s35+p1+p2+p3)/6.5)
    print(" |         CGPA             = ",((s1+s2+s3+s4+s35+p1+p2+p3)/65)+0.75,"\n ----------------------------")
   else:
    print(" |      RESULT IS FAIL      | \n ----------------------------")
  if ch1==2 and ch2==2:#selection of 4th sem 2nd year
   sem4()
   print("\n\n ----------------------------\n |          RESULT          | \n ----------------------------")
   ch3=int(ch1)+int(ch2)
   print(" |   Of",ch1,"year",ch3,"semester   |" )
   if s1 and s2 and s3 and s4 and s45>= 40 and p1 and p2>= 30 : # calling variables from sem4()
    print(" |      RESULT IS PASS      | ")
    print(" |      PERCENTAGE %        = ",(s1+s2+s3+s4+s45+p1+p2)/6)
    print(" |         CGPA             = ",((s1+s2+s3+s4+s45+p1+p2)/60)+0.75,"\n ----------------------------")
   else:
    print(" |      RESULT IS FAIL      | \n ----------------------------")
 if ch1==3: # Result calc 3rd year
  print("\n\n ----------------------------\n | List of semester      no.| \n ----------------------------\n | Fifth sem           -> 1.| \n | Sixth sem           -> 2.| ")
  ch2=int(input(" | Enter your choice = "))
  while ch2>2:
     print(" | Invalid input")
     ch2=int(input(" | Enter your choice = "))
  if ch1==3 and ch2==1:#selection of 5st sem 3rd year
   sem5()
   print("\n\n ----------------------------\n |          RESULT          | \n ----------------------------")
   ch3=int(ch1)+int(ch2)+1
   print(" |   Of",ch1,"year",ch3,"semester   |" )
   if s1 and s2 and s3 and s4 >= 40 and p1 and p2 and p3 and s55>= 30 : # calling variables from sem5()
    print(" |      RESULT IS PASS      | ")
    print(" |      PERCENTAGE %        = ",(s1+s2+s3+s4+s55+p1+p2+p3)/6)
    print(" |         CGPA             = ",((s1+s2+s3+s4+s55+p1+p2+p3)/60)+0.75,"\n ----------------------------")
   else:
    print(" |      RESULT IS FAIL      | \n ----------------------------")
  if ch1==3 and ch2==2:#selection of 6th sem 3rd year
   sem6()
   print("\n\n ----------------------------\n |          RESULT          | \n ----------------------------")
   ch3=int(ch1)+int(ch2)+1
   print(" |   Of",ch1,"year",ch3,"semester   |" )
   if s1 and s2 and s3 and s4 >= 40 and p1 and p2 and s65>= 30 :  # calling variables from sem6()
    print(" |      RESULT IS PASS      | ")
    print(" |      PERCENTAGE %        = ",(s1+s2+s3+s4+s65+p1+p2)/5.5)
    print(" |         CGPA             = ",((s1+s2+s3+s4+s65+p1+p2)/55)+0.75,"\n ----------------------------")
   else:
    print(" |      RESULT IS FAIL      | \n ----------------------------")
 if ch1==4:
  print("\n\n ----------------------------\n | List of semester      no.| \n ----------------------------\n | Seventh sem         -> 1.| \n | Eighth sem          -> 2.| ")
  ch2=int(input(" | Enter your choice = "))
  while ch2>2 :
     print(" | Invalid input")
     ch2=int(input(" | Enter your choice = "))
  if ch1==4 and ch2==1:#selection of 7th sem 4th year
   sem7()
   print("\n\n ---------------------------\n |          RESULT          | \n ----------------------------")
   ch3=int(ch1)+int(ch2)+1
   print(" |   Of",ch1,"year",ch3,"semester   |" )
   if s1 and s2 and s3 and s4 and s75>= 40 and p1 and p2 and p3>= 30 : # calling variables from sem7()
    print(" |      RESULT IS PASS      | ")
    print(" |      PERCENTAGE %        = ",(s1+s2+s3+s4+s75+p1+p2+p3)/6.5)
    print(" |         CGPA             = ",((s1+s2+s3+s4+s75+p1+p2+p3)/65)+0.75,"\n ----------------------------")
   else:
    print(" |      RESULT IS FAIL      | \n ----------------------------")
  if ch1==4 and ch2==2:#selection of 8th sem 4th year
   sem8()
   print("\n\n ----------------------------\n |          RESULT          | \n ----------------------------")
   ch3=int(ch1)+int(ch2)+2
   print(" |   Of",ch1,"year",ch3,"semester   |" )
   if s1 and s2 and s3 and s4 and s85>= 40 and p1 and p2>= 30 and p3>=60: # calling variables from sem7()
    print(" |      RESULT IS PASS      | ")
    print(" |      PERCENTAGE %        = ",(s1+s2+s3+s4+s85+p1+p2+p3)/7.5)
    print(" |         CGPA             = ",((s1+s2+s3+s4+s85+p1+p2+p3)/75)+0.75,"\n ----------------------------")
   else:
    print(" |      RESULT IS FAIL      | \n ----------------------------")
main()# calling main function
print("\n | Do you want to calculate |")
more=str(input(" | any other result (Y/N) = "))#for calculating more result  
while more=="y"and"Y":
 main()# calling main function
 print("\n | Do you want to calculate |")
 more=str(input(" | any other result (Y/N) = ")) 
 if more=="N" and "n":
  break# break to end program

